{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "api",
      "image": "technologiestiftung/flusshygiene-postgres-api:v1.9.13-dev",
      "hostname": "api",
      "essential": false,
      "memory": 8192
    },
    {
      "name": "ocpu",
      "image": "technologiestiftung/flusshygiene-opencpu-fhpredict-api:v0.7.0-dev",
      "hostname": "ocpu",
      "essential": false,
      "memory": 4096
    },
    {
      "name": "spa",
      "image": "technologiestiftung/flusshygiene-cms-spa:v0.12.1-dev",
      "hostname": "spa",
      "essential": false,
      "memory": 1024
    },
    {
      "name": "middlelayer",
      "image": "technologiestiftung/flusshygiene-opencpu-middlelayer:v0.4.1-dev",
      "hostname": "middlelayer",
      "essential": false,
      "memory": 512
    },
    {
      "name": "nginx",
      "image": "technologiestiftung/flusshygiene-nginx-gateway:v1.3.0-dev",
      "hostname": "nginx",
      "essential": true,
      "memory": 1024,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        },
        {
          "hostPort": 443,
          "containerPort": 80
        }
      ],
      "links": ["api", "spa", "ocpu", "middlelayer"]
    }
  ]
}
