{
  "name": "@tsb/postgres-api",
  "version": "2.13.0",
  "private": true,
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "ts-node": "ts-node src/index.ts",
    "test": "jest --runInBand",
    "test:watch": "jest --runInBand --watch",
    "testWithCoverage": "jest --coverage --setupFiles ./__tests__/setupFiles/config",
    "populatedb": "node dist/setup/populate-db.js",
    "dev": "npm run clean && nodemon",
    "dev:populate": "npm run clean && POPULATE=true NODE_ENV=development nodemon",
    "clean": "rimraf ./dist && mkdirp ./dist",
    "dev:docker": "nodemon --config nodemon.docker.json",
    "build": "tsc -p .",
    "start": "node -r dotenv/config dist/index.js",
    "debug": "ndb node -r dotenv/config dist/index.js",
    "majestic": "majestic",
    "typeorm": "typeorm",
    "prettier": "prettier \"**/*.+(js|json|yml|yaml|ts)\"",
    "prettier:list": "npm run prettier -- --list-different",
    "prettier:write": "npm run prettier -- --write",
    "validate": "npm run build",
    "lint-staged": "lint-staged",
    "lint": "eslint './src/**/*.ts'",
    "playground": "npm run build && node -r dotenv/config"
  },
  "repository": {
    "type": "git",
