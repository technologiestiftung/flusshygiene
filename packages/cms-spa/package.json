    "@types/lodash.differenceby": "4.8.6",
    "@types/lodash.throttle": "4.1.6",
    "@types/node": "12.12.29",
    "@types/papaparse": "5.0.2",
    "@types/react": "16.9.32",
    "@types/react-copy-to-clipboard": "4.3.0",
    "@types/react-csv": "1.1.1",
    "@types/react-dom": "16.9.4",
    "@types/react-helmet": "5.0.14",
    "@types/react-map-gl": "5.0.3",
    "@types/react-router-dom": "5.1.3",
    "@types/react-table": "7.0.4",
    "@types/turndown": "5.0.0",
    "@types/yup": "0.26.26",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-macros": "2.8.0",
    "customize-cra": "0.9.1",
    "cz-conventional-changelog": "3.2.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react-hooks": "4.0.4",
    "http-proxy-middleware": "1.0.4",
    "husky": "4.2.5",
    "jest-fetch-mock": "3.0.3",
    "lint-staged": "10.2.6",
    "node-sass": "7.0.0",
    "prettier": "2.0.5",
    "react-app-rewired": "2.1.6",
    "react-scripts": "3.4.1",
    "react-test-renderer": "16.13.1",
    "require-context.macro": "1.2.2",
    "ts-jest": "26.0.0",
    "ts-node": "8.10.1",
    "typescript": "3.9.3"
  },
  "resolutions": {
    "react-helmet/react-side-effect": "2.1.0"
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/__test-utils",
      "!**/coverage/**",
      "!src/setup/**",
      "!src/react-auth0-wrapper.tsx",
      "!src/serviceWorker.ts",
      "!src/stories/**"
    ]
  },
  "proxy": "http://localhost:5004",
  "husky": {
    "hooks": {
      "pre-commit": "npm run add:version && npm run lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
