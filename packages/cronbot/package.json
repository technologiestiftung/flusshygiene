{
  "name": "@tsb/cronbot",
  "version": "2.12.0",
  "private": true,
  "description": "A cron worker for the Flusshygiene project",
  "keywords": [
    "cron",
    "flusshygiene",
    "worker"
  ],
  "author": "fabianmoronzirfas <fabianmoronzirfas@protonmail.ch>",
  "homepage": "https://github.com/technologiestiftung/flusshygiene/tree/master/packages/cronbot#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "directories": {
    "lib": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "cronbot": "./dist/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/technologiestiftung/flusshygiene.git"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc -p .",
    "just": "just",
    "jest": "jest",
    "dev": "just dev",
    "start": "just start",
    "test:ci": "just test",
    "clean": "rimraf dist && mkdirp dist",
    "ts:watch": "npm run clean && tsc -p . --watch",
    "test": "jest",
    "change": "beachball change",
    "plop": "plop",
    "ts-node": "ts-node src/cli.ts",
    "test:watch": "jest --watch",
    "prettier": "prettier \"**/*.+(js|json|yml|yaml|ts)\"",
    "prettier:list": "npm run prettier -- --list-different",
    "prettier:write": "npm run prettier -- --write",
    "validate": "npm run build",
    "lint-staged": "lint-staged",
    "lint": "eslint './src/**/*.ts'"
  },
  "bugs": {
    "url": "https://github.com/technologiestiftung/flusshygiene/issues"
  },
  "devDependencies": {
    "@types/dotenv": "8.2.0",
    "@types/jest": "24.0.24",
    "@types/lodash": "4.14.149",
    "@types/lowdb": "1.0.9",
    "@types/meow": "5.0.0",
    "@types/mock-fs": "4.10.0",
    "@types/node": "12.12.29",
    "@types/nodemailer": "6.4.0",
    "@types/shortid": "0.0.29",
    "@typescript-eslint/eslint-plugin": "2.12.0",
    "@typescript-eslint/parser": "2.12.0",
    "beachball": "1.16.0",
    "cz-conventional-changelog": "3.0.2",
    "dotenv-flow": "3.1.0",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "just-scripts": "0.36.1",
    "lint-staged": "9.5.0",
    "mkdirp": "0.5.2",
    "mock-fs": "4.10.4",
    "nock": "11.7.2",
    "nodemon": "2.0.2",
    "plop": "2.5.3",
    "prettier": "1.19.1",
    "rimraf": "3.0.0",
    "ts-jest": "24.2.0",
    "ts-node": "8.6.2",
    "typescript": "3.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate && npm run lint-staged"
    }
  },
  "dependencies": {
    "dotenv": "8.2.0",
    "form-data": "3.0.0",
    "got": "10.2.2",
    "lodash.uniqby": "4.7.0",
    "lowdb": "1.0.0",
    "meow": "6.0.0",
    "nodemailer": "6.4.3",
    "shortid": "2.2.15"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
