{
  "name": "@tsb/cronbot",
  "version": "2.3.2",
  "description": "A cron worker for the Flusshygiene project",
  "keywords": [
    "cron",
    "flusshygiene",
    "worker"
  ],
  "author": "fabianmoronzirfas <fabianmoronzirfas@protonmail.ch>",
  "homepage": "https://github.com/technologiestiftung/flusshygiene/tree/master/packages/cronbot#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/technologiestiftung/flusshygiene.git"
  },
  "scripts": {
    "ts-node": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "tsc -p .",
    "start": "node -r dotenv/config dist/index.js",
    "prettier": "prettier \"**/*.+(js|json|yml|yaml|ts)\"",
    "prettier:list": "npm run prettier -- --list-different",
    "prettier:write": "npm run prettier -- --write",
    "validate": "npm run build",
    "lint-staged": "lint-staged",
    "lint": "eslint './src/**/*.ts'"
  },
  "bugs": {
    "url": "https://github.com/technologiestiftung/flusshygiene/issues"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^24.0.24",
    "@types/meow": "^5.0.0",
    "@types/node": "^12.12.21",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate && npm run lint-staged"
    }
  },
  "dependencies": {
    "dotenv": "^8.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
